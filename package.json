{
  "name": "react-utility-yard",
  "version": "1.0.9",
  "description": "A react utility Package",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "prebuild" : "",
    "build": "rimraf ./dist && npm run build:esm && npm run build:cjs ",
    "build:linux" : "rm -rf ./dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc && copyfiles -u 1 src/assets/* dist/esm && npx tailwindcss -i ./src/tailwind.css -o ./dist/esm/tailwind.css",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && copyfiles -u 1 src/assets/* dist/cjs && npx tailwindcss -i ./src/tailwind.css -o ./dist/cjs/tailwind.css",
    "test": "echo \"Error: no test specified\" && exit 0",
    "storybook": "storybook dev -p 6006",
    "storybook-prod": "start-storybook -p 6006 -s public",
    "build-storybook": "storybook build",
    "build-storybook-prod": "build-storybook -s public",
    "serve-storybook": "serve storybook-static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Infinity8sailor/react-utility-yard.git"
  },
  "keywords": [
    "React",
    "Utility"
  ],
  "author": "Vaibhav Dasharathe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Infinity8sailor/react-utility-yard/issues"
  },
  "homepage": "https://github.com/Infinity8sailor/react-utility-yard#readme",
  "dependencies": {
    "@testing-library/react": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.2",
    "@storybook/addon-interactions": "^7.4.2",
    "@storybook/addon-links": "^7.4.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.4.2",
    "@storybook/react": "^7.4.2",
    "@storybook/react-vite": "^7.4.2",
    "@storybook/testing-library": "^0.2.1",
    "@storybook/theming": "^7.4.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "copyfiles": "^2.4.1",
    "postcss": "^8.4.29",
    "prop-types": "^15.8.1",
    "storybook": "^7.4.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
